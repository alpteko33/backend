{
  "version": 2,
  "name": "avukatlik-buros-yonetim-api",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "installCommand": "npm ci",
  "buildCommand": "npx prisma generate"
} 